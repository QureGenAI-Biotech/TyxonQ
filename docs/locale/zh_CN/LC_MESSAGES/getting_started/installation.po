msgid "Windows"
msgstr ""

#: 0bfa7269431f4f97ac425e6fd597e93c
msgid ""
"# Ensure Python is installed (download from python.org)\n"
"\n"
"# Install TyxonQ\n"
"pip install tyxonq"
msgstr ""

#: 3fee2a3ca5a549b1b51d27e07143323a
msgid "For GPU support on Windows:"
msgstr ""

#: e758d86a2a4148888f97b9f3d524e281
msgid ""
"# Install CUDA toolkit from NVIDIA\n"
"# Then install TyxonQ with GPU support\n"
"pip install tyxonq[gpu]"
msgstr ""

#: 8bf026ad2fe745ba86ef1111fc78cf21
msgid "Development Installation"
msgstr ""

#: abf330b5c5c54b539a0b7bc55f14247d
msgid "To contribute to TyxonQ or use the latest development version:"
msgstr ""

#: 61cbd40c1723405eb30a43eb827838fe
msgid ""
"# Clone the repository\n"
"git clone https://github.com/QureGenAI-Biotech/TyxonQ.git\n"
"cd TyxonQ\n"
"\n"
"# Install in editable mode with development dependencies\n"
"pip install -e \".[dev]\"\n"
"\n"
"# Run tests to verify installation\n"
"pytest tests/"
msgstr ""

#: b29d34738a0a41d5be6b7c73119df606
msgid "Docker Installation"
msgstr ""

#: 964a5c6371b34b3d9e4cb8ea9033ce76
msgid "TyxonQ provides Docker images for reproducible environments:"
msgstr ""

#: f41a1a16ba1e494da9a1b4514e1491a0
msgid ""
"# Pull the latest image\n"
"docker pull quregenai/tyxonq:latest\n"
"\n"
"# Run TyxonQ in a container\n"
"docker run -it quregenai/tyxonq:latest python\n"
"\n"
"# With GPU support\n"
"docker run --gpus all -it quregenai/tyxonq:gpu python"
msgstr ""

#: 0fe5fceed5934565b543a714d415ec21
msgid "Conda Installation"
msgstr ""

#: 0884682384bb4ce8b6615c39b2633392
msgid "TyxonQ can be installed via Conda:"
msgstr ""

#: 5e629a7291a84544b529bee8732fa633
msgid ""
"# Create a new environment\n"
"conda create -n tyxonq python=3.10\n"
"conda activate tyxonq\n"
"\n"
"# Install TyxonQ\n"
"conda install -c conda-forge tyxonq"
msgstr ""

#: 8846e14948af49c8bbedd5ac525aeeb2
msgid "Verifying Installation"
msgstr ""

#: 8a284ae45b4c46f28b2058bdee605944
msgid "After installation, verify that TyxonQ is working correctly:"
msgstr ""

#: cd145215daef4bb78beb6f34cf6debbc
msgid ""
"import tyxonq as tq\n"
"\n"
"# Check version\n"
"print(tq.__version__)\n"
"\n"
"# Create a simple circuit to test\n"
"circuit = tq.Circuit(2)\n"
"circuit.h(0)\n"
"circuit.cnot(0, 1)\n"
"\n"
"# Run on simulator\n"
"result = circuit.compile().device('statevector').run(shots=100)\n"
"print(\"Installation successful!\")\n"
"print(result.counts)"
msgstr ""

#: b662f7e8184a4b748fb208adf59b40d3
msgid "Expected output:"
msgstr ""

#: 0d70b7fb4f5c45ceb5ad89b115a0e36c
